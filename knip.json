{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "ignore": [
        "**/test/**/{fixtures,_temp-fixtures}/**"
    ],
    "workspaces": {
        "packages/astro": {
            "entry": [
                "index.d.ts",
                "src/core/index.ts",
                "env.d.ts",
                "src/env/runtime.ts",
                "src/env/setup.ts",
                "types.d.ts",
                "client.d.ts",
                "astro-jsx.d.ts",
                "tsconfigs/*.json",
                "src/jsx/rehype.ts",
                "jsx-runtime.d.ts",
                "src/jsx-runtime/index.ts",
                "src/runtime/**/*.ts",
                "src/config/entrypoint.ts",
                "src/container/index.ts",
                "src/core/app/index.ts",
                "src/core/app/node.ts",
                "components/*",
                "src/toolbar/index.ts",
                "src/actions/runtime/**/*.ts",
                "src/assets/index.ts",
                "src/assets/runtime.ts",
                "src/assets/utils/index.ts",
                "src/assets/endpoint/**/*.ts",
                "src/assets/services/sharp.ts",
                "src/assets/services/noop.ts",
                "src/content/loaders/index.ts",
                "src/content/runtime.ts",
                "src/content/runtime-assets.ts",
                "zod.d.ts",
                "zod.mjs",
                "src/core/errors/userError.ts",
                "src/core/middleware/index.ts",
                "src/virtual-modules/**/*.ts",
                "astro.js",
                "{test,e2e}/**/*.test.js",
                "test/types/**/*",
                "templates/**/*"
            ],
            "ignore": [
                "**/e2e/**/{fixtures,_temp-fixtures}/**",
                "performance/**/*"
            ],
            "ignoreDependencies": ["rehype-autolink-headings", "rehype-slug", "rehype-toc", "remark-code-titles"]
        },
        "packages/integrations/cloudflare": {
            "entry": [
                "src/index.ts",
                "src/entrypoints/*.ts",
                "test/**/*.test.js"
            ],
            "ignoreDependencies": ["cloudflare"]
        },
        "packages/integrations/markdoc": {
            "entry": [
                "src/extensions/*.ts",
                "src/{config,index,runtime,runtime-assets-config}.ts",
                "src/components/index.ts",
                "test/**/*.test.js"
            ]
        },
        "packages/integrations/mdx": {
            "entry": [
                "src/{index,server}.ts",
                "test/**/*.test.js"
            ],
            "ignoreDependencies": ["@types/*"]
        },
        "packages/integrations/netlify": {
            "entry": [
                "src/{index,static,functions,ssr-function,image-service}.ts",
                "test/**/*.test.js"
            ],
            "ignore": ["test/hosted/**"]
        },
        "packages/integrations/node": {
            "entry": [
                "src/{index,server,preview}.ts",
                "test/**/*.test.js"
            ]
        },
        "packages/integrations/partytown": {
            "entry": ["src/index.ts"]
        },
        "packages/integrations/preact": {
            "entry": [
                "src/{index,client,client-dev,server}.ts"
            ]
        },
        "packages/integrations/react": {
            "entry": [
                "src/{index,actions,client,client-v17,server,server-v17,jsx-runtime}.ts",
                "test/**/*.test.js"
            ]
        },
        "packages/integrations/sitemap": {
            "entry": [
                "src/index.ts",
                "test/**/*.test.js"
            ]
        },
        "packages/integrations/solid": {
            "entry": [
                "src/{index,client,server}.ts",
                "test/**/*.test.js"
            ]
        },
        "packages/integrations/svelte": {
            "entry": [
                "src/{index,client,server}.ts",
                "src/editor.cts"
            ]
        },
        "packages/integrations/vercel": {
            "entry": [
                "src/index.ts",
                "src/serverless/{entrypoint,adapter}.ts",
                "src/static/adapter.ts",
                "src/image/{build-service,dev-service}.ts",
                "test/**/*.test.js"
            ],
            "ignore": ["test/hosted/**"]
        },
        "packages/integrations/vue": {
            "entry": [
                "src/{index,client,server}.ts",
                "src/editor.cts",
                "test/**/*.test.js"
            ]
        },
        "packages/integrations/web-vitals": {
            "entry": [
                "src/{index,middleware,endpoint,client-script,db-config}.ts",
                "test/**/*.test.js"
            ]
        }
    }
}